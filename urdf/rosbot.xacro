<?xml version='1.0'?>

<robot name="rosbot"
  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:arg name="robot_namespace" default=""/>

  <xacro:property name="cameraSize" value="0.05"/>
  <xacro:property name="cameraMass" value="0.1"/>

  <xacro:include filename="$(find rosbot_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find assignment_1)/urdf/rosbot.gazebo" />

  <link name="base_link"></link>
  <joint name="base_link_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="body_link"/>
  </joint>

  <link name='body_link'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="5"/>
      <origin xyz="0 0 0.04" rpy=" 0 0 0"/>
      <inertia
      ixx="0.01" ixy="0.01" ixz="0"
      iyy="0.01" iyz="0.01"
      izz="0.01"
      />
    </inertial>


    <collision>
      <origin xyz="0 0 0.02" rpy="0 0 0"/>
      <geometry>
        <box size="0.20 0.15 0.04"/>
      </geometry>
    </collision>

    <visual>
      <origin xyz="0 0 -0.02" rpy=" 1.5707 0 1.5707"/>
      <geometry>
        <mesh filename="package://rosbot_description/meshes/box.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="White"/>
    </visual>

  </link>

  <joint name="top_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="body_link"/>
    <child link="top"/>
  </joint>

  <link name='top'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="0.01"/>
      <origin xyz="0 0 0.055" rpy=" 0 0 0"/>
      <inertia
      ixx="0.0" ixy="0" ixz="0"
      iyy="0.0" iyz="0"
      izz="0.0"
      />
    </inertial>
    <visual>
      <origin xyz="0 0 0.055" rpy=" 1.5707 0 1.5707"/>
      <geometry>
        <mesh filename="package://rosbot_description/meshes/upper.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="Red"/>
    </visual>
  </link>

  <link name="front_left_wheel">
    <collision>
      <origin xyz="0 -0.014 0" rpy="${pi/2} 0 0"/>
      <geometry>
        <cylinder length="0.036" radius="0.04"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>10</mu>
            <mu2>10</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://rosbot_description/meshes/wheel.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="Black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="0.5"/>
      <inertia
      ixx="0.001" ixy="0.0" ixz="0.0"
      iyy="0.001" iyz="0.0"
      izz="0.001"/>
    </inertial>
  </link>

  <link name="front_right_wheel">
    <collision>
      <origin xyz="0 0.014 0" rpy="${pi/2} 0 0"/>
      <geometry>
        <cylinder length="0.036" radius="0.04"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>10</mu>
            <mu2>10</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.5707"/>
      <geometry>
        <mesh filename="package://rosbot_description/meshes/wheel.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="Black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="0.5"/>
      <inertia
      ixx="0.001" ixy="0.0" ixz="0.0"
      iyy="0.001" iyz="0.0"
      izz="0.001"/>
    </inertial>
  </link>

  <link name="rear_left_wheel">
    <collision>
      <origin xyz="0 -0.014 0" rpy="${pi/2} 0 0"/>
      <geometry>
        <cylinder length="0.036" radius="0.04"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>10</mu>
            <mu2>10</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://rosbot_description/meshes/wheel.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="Black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="0.5"/>
      <inertia
      ixx="0.001" ixy="0.0" ixz="0.0"
      iyy="0.001" iyz="0.0"
      izz="0.001"/>
    </inertial>
  </link>

  <link name="rear_right_wheel">
    <collision>
      <origin xyz="0 0.014 0" rpy="${pi/2} 0 0"/>
      <geometry>
        <cylinder length="0.036" radius="0.04"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>10</mu>
            <mu2>10</mu2>
          </ode>
        </friction>
      </surface>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 -1.5707"/>
      <geometry>
        <mesh filename="package://rosbot_description/meshes/wheel.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="Black"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <mass value="0.5"/>
      <inertia
      ixx="0.001" ixy="0.0" ixz="0.0"
      iyy="0.001" iyz="0.0"
      izz="0.001"/>
    </inertial>
  </link>


  <joint type="continuous" name="front_left_wheel_hinge">
    <origin xyz="0.05 0.11 0" rpy="0 0 0"/>
    <child link="front_left_wheel"/>
    <parent link="body_link"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="1" velocity="10"/>
    <joint_properties damping="5.0" friction="1.0"/>
  </joint>

  <transmission name="front_left_wheel_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="front_left_wheel_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="front_left_wheel_hinge">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <joint type="continuous" name="front_right_wheel_hinge">
    <origin xyz="0.05 -0.11 0" rpy="0 0 0"/>
    <child link="front_right_wheel"/>
    <parent link="body_link"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="1" velocity="10"/>
    <joint_properties damping="5.0" friction="1.0"/>
  </joint>

  <transmission name="front_right_wheel_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="front_right_wheel_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="front_right_wheel_hinge">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <joint type="continuous" name="rear_left_wheel_hinge">
    <origin xyz="-0.055 0.11 0" rpy="0 0 0"/>
    <child link="rear_left_wheel"/>
    <parent link="body_link"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="1" velocity="10"/>
    <joint_properties damping="5.0" friction="1.0"/>
  </joint>

  <transmission name="rear_left_wheel_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="rear_left_wheel_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="rear_left_wheel_hinge">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <joint type="continuous" name="rear_right_wheel_hinge">
    <origin xyz="-0.055 -0.11 0" rpy="0 0 0"/>
    <child link="rear_right_wheel"/>
    <parent link="body_link"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="1" velocity="10"/>
    <joint_properties damping="5.0" friction="1.0"/>
  </joint>

  <transmission name="rear_right_wheel_trans" type="SimpleTransmission">
    <type>transmission_interface/SimpleTransmission</type>
    <actuator name="rear_right_wheel_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    <joint name="rear_right_wheel_hinge">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
  </transmission>

  <!-- Orbbec Astra RGBD Camera -->


  <joint name="joint1" type="continuous">
    <origin xyz="-0.038 0 0.123" rpy="0 0 0"/>
    <parent link="body_link"/>
    <child link="link1"/>
    <axis rpy="0 0 0" xyz="0 0 1"/>
    <limit effort="1000.0" velocity="0.2"/>
    <!--limit lower="-3.14" upper="3.14" effort="1000.0" velocity="0.2"/-->
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <link name="link1">
      <collision>
        <origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry>
          <!--box size="0.01 0.01 0.5"/-->
          <cylinder radius="0.02" length="0.5"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0.25" rpy="0 0 0"/>
        <geometry>
    <!--box size="0.01 0.01 0.5"/-->
    <cylinder radius="0.02" length="0.5"/>
        </geometry>
        <material name="White">
    <color rgba="1 0 0 1"/>
        </material>
      </visual>
      <inertial>
        <mass value="0.025" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.000010279" ixy="0" ixz="0" iyy="0.000010279" iyz="0" izz="0.000007225"/>
      </inertial>
  </link>

  <link name="sphere1">
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
    <sphere radius="0.03"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
    <sphere radius="0.03"/>
        </geometry>
        <material name="White">
    <color rgba="1 0 0 1"/>
        </material>
      </visual>
      <inertial>
        <mass value="0.001" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.000010279" ixy="0" ixz="0" iyy="0.000010279" iyz="0" izz="0.000010279"/>
      </inertial>
  </link>
  <joint name="joint_sphere" type="fixed">
    <origin xyz="0 0 0.5" rpy="0 0 0"/>
    <parent link="link1"/>
    <child link="sphere1"/>
  </joint>



  <link name="link2">
      <collision>
        <origin xyz="0 0 0.6" rpy="0 0 0"/>
        <geometry>
    <cylinder radius="0.01" length="1.2"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="0 0 0.6" rpy="0 0 0"/>
        <geometry>
    <cylinder radius="0.01" length="1.2"/>
        </geometry>
        <material name="White">
    <color rgba="1 0 0 1"/>
        </material>
      </visual>
      <inertial>
        <mass value="0.005" />
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <inertia ixx="0.000010279" ixy="0" ixz="0" iyy="0.000010279" iyz="0" izz="0.000010279"/>
      </inertial>
  </link>

  <joint name="joint2" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="sphere1"/>
    <child link="link2"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
      <limit lower="-1.57" upper="1.57" effort="1000.0" velocity="0.2"/>
    <!--limit lower="-3.14" upper="3.14" effort="1000.0" velocity="0.2"/-->
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>


  <link name="camera_link">
	    <collision>
	      <origin xyz="0 0 0.015" rpy="0 0 0"/>
	      <geometry>
		<box size="0.03 0.03 0.03"/>
	      </geometry>
	    </collision>
	    <visual>
	      <origin xyz="0 0 0.015" rpy="0 0 0"/>
	      <geometry>
		<box size="0.03 0.03 0.03"/>
	      </geometry>
	      <material name="Red">
		<color rgba="1 0 0 1"/>
	      </material>
	    </visual>
	    <inertial>
	      <mass value="1e-5" />
	      <origin xyz="0 0 0" rpy="0 0 0"/>
	      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
	    </inertial>
  </link>
  <joint name="joint_camera" type="revolute">
      <axis rpy="0 0 0" xyz="0 1 0"/>
      <limit lower="-3.14" upper="3.14" effort="1000.0" velocity="0.2"/>
      <joint_properties damping="1.0" friction="1.0"/>
      <origin xyz="0 0 1.2" rpy="0 0 0"/>
      <parent link="link2"/>
      <child link="camera_link"/>
  </joint>


  <link name="hokuyo_link">
     <collision>
       <origin xyz="0 0 0.025" rpy="0 0 0"/>
       <geometry>
   <box size="0.03 0.03 0.03"/>
       </geometry>
     </collision>
     <visual>
       <origin xyz="0 0 0.025" rpy="0 0 0"/>
       <geometry>
   <box size="0.03 0.03 0.03"/>
       </geometry>
       <material name="Black">
   <color rgba="1 0 0 1"/>
       </material>
     </visual>
     <inertial>
       <mass value="1e-5" />
       <origin xyz="0 0 0" rpy="0 0 0"/>
       <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
     </inertial>
  </link>
  <joint name="hokuyo_joint" type="fixed">
     <axis xyz="0 1 0" />
     <origin xyz="0.06 0 0.057" rpy="0 0 0"/>
     <parent link="body_link"/>
     <child link="hokuyo_link"/>
  </joint>

  <!-- Trasmission for camera rotation joint-->
<transmission name="trans_joint1">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="joint1">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
  </joint>
  <actuator name="motor_joint1">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>

<transmission name="trans_joint2">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="joint2">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
  </joint>
  <actuator name="motor_joint2">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>

<transmission name="trans_joint_camera">
  <type>transmission_interface/SimpleTransmission</type>
  <joint name="joint_camera">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
  </joint>
  <actuator name="motor_joint_camera">
    <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    <mechanicalReduction>1</mechanicalReduction>
  </actuator>
</transmission>


</robot>
